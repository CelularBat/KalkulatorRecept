    <script>
        function LogOut() {
            fetch("/api/logout", {
              method: "POST",
              body: JSON.stringify({
                user: "<%= USER %>"
              }),
              headers: {
                "Content-type": "application/json; charset=UTF-8"
              }
            })
            .then( ()=> {
                location.reload();
            } );
            
        }
    </script>
<p>Hello <%= USER %>, you are logged in!</p>
<button type='button' onclick="LogOut()">Log Out</button>